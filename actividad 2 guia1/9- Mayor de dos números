public class Ejercicio9 {
    public static int mayorDeDos(int num1, int num2) {
        if (num1 > num2) {
            return num1;
        } else {
            return num2;
        }
    }

    public static void main(String[] args) {
        int num1 = 5;
        int num2 = 8;
        
        int mayor = mayorDeDos(num1, num2);
        
        System.out.println("El mayor n√∫mero es: " + mayor);
    }
}
